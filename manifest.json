{
  "name": "Bork.ly",
  "options_page": "options.html",
  "version": "1.0",
  "description": "A miniature spreadsheet for quick calculations",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/cloudy16.png",
      "32": "images/cloudy32.png",
      "48": "images/cloudy48.png",
      "128": "images/cloudy128.png"
    }
  },
  "icons": {
    "16": "images/cloudy16.png",
    "32": "images/cloudy32.png",
    "48": "images/cloudy48.png",
    "128": "images/cloudy128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-l0OHvOQ2TXE6F5u/nsNYRts1EfrSsvcPC096y4yRACo='; object-src 'self'",
  "manifest_version": 2
}
